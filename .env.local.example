# NextAuth.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-here

# GitHub OAuth App
AUTH_GITHUB_ID=your-github-client-id
AUTH_GITHUB_SECRET=your-github-client-secret

# Allowed User (only this user can access the dashboard)
ALLOWED_USER_EMAIL=your@email.com
ALLOWED_GITHUB_USERNAME=yourusername

# GitHub API Token (for managing repo content)
GITHUB_TOKEN=your-github-personal-access-token

# GitHub Repository (format: username/repo)
GITHUB_REPO=your-username/your-repo-name